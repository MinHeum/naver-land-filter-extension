{
  "name": "naver-land-filter",
  "version": "2.0.0",
  "description": "네이버 부동산 지도에서 추가 필터링 기능을 제공하는 Chrome 확장 프로그램",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build && node scripts/copy-assets.js",
    "build:extension": "yarn build && echo 'Extension ready in dist/ folder'",
    "clean": "node scripts/clean.js",
    "analyze": "yarn build && node scripts/analyze.js",
    "preview": "vite preview",
    "lint": "eslint src/**/*.js scripts/**/*.js",
    "lint:fix": "eslint src/**/*.js scripts/**/*.js --fix",
    "test": "vitest"
  },
  "keywords": [
    "chrome-extension",
    "naver",
    "real-estate",
    "filter",
    "korea",
    "부동산",
    "네이버",
    "필터"
  ],
  "author": "Minheum <plutokor@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MinHeum/naver-land-filter-extension.git"
  },
  "bugs": {
    "url": "https://github.com/MinHeum/naver-land-filter-extension/issues"
  },
  "homepage": "https://github.com/MinHeum/naver-land-filter-extension#readme",
  "devDependencies": {
    "eslint": "^8.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^2.8.0",
    "vite": "^5.0.0",
    "vite-plugin-web-extension": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "Chrome >= 88"
  ],
  "packageManager": "yarn@4.9.2"
}
